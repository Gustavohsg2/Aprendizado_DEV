{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "input-button-name": "off",
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-required-parent": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "meta-viewport": "off"
  }
}